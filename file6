//Write a program to reverse an array or string

#include<iostream>
using namespace std;
int main()
{ 
    int n=0;
    int num[n];
    cout<<"enter the array size"<<endl;
    cin>>n;
   cout<<"enter elements of array";
   for(int i=0;i<n;i++)
   cin>>num[i];
   cout<<" array is ";
   for(int i=0;i<n;i++)
   { cout<<num[i]<<endl;}
   cout<<"reverse array is"<<endl;
    for(int i=n-1;i>=0;i--)
    cout<<num[i]<<endl;
return 0;
}
