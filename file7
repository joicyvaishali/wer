// array rotation

#include<iostream>
#include<climits>
using namespace std;
int main()
{
    int n;
    int num[n-1];
    int x = num[n-1];


    cout<<"enter size";
    cin>>n;
    cout<<"enter elements";
    for(int i=0;i<n;i++)
    cin>>num[i];
    cout<<"elemets are"<<endl;
    for(int i=0;i<n;i++)
    cout<<num[i]<<endl;
    cout<<"array rotation";
    for(int i=n-1;i>0;i--)
    {num[i]=num[i-1];}
    num[0]=x;
    cout<<"rotated array"<<endl;
     for(int i=0;i<n;i++)
    cout<<num[i]<<endl;
    return 0;
}
