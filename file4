fixed point in a given array


#include<iostream>
using namespace std;
int num(int arr[],int n,int k)
{
    for(int i=0;i<n;i++)
   { if(arr[i]==k)
    return i ;
}
return -1;}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int k;
    cin>>k;
    cout<<num(arr,n,k)<<" ";

    return 0;
}
